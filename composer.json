{
    "name": "tillikum/tillikum-core-module",
    "description": "The Tillikum Project core module",
    "homepage": "http://tillikum.org/",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Gordon Stratton",
            "email": "gordon.stratton@gmail.com",
            "role": "Lead developer"
        }
    ],
    "autoload": {
        "psr-0": {
            "Tillikum\\": "src/",
            "Tillikum_": "src/"
        }
    },
    "require": {
        "beberlei/DoctrineExtensions": "dev-master",
        "doctrine/orm": "~2.3",
        "guilhermeblanco/zendframework1-doctrine2": "dev-master",
        "gwis/php-valueobjects": "dev-master",
        "php": ">=5.3.3",
        "tillikum/zf2-cas-authentication-adapter": "dev-master",
        "zendframework/zendframework1": "~1.12",
        "zendframework/zendframework": "~2.2"
    },
    "require-dev": {
        "doctrine/data-fixtures": "dev-master",
        "phing/phing": "~2.5.0",
        "phpunit/phpunit": "~3.7.0"
    },
    "suggest": {
        "ext-apc": "A free and open opcode cache for PHP. Very important for performance, and useful for caching.",
        "ext-memcached": "PHP extension for interfacing with memcached via libmemcached library."
    },
    "support": {
        "irc": "irc://chat.freenode.net/tillikum",
        "issues": "https://github.com/tillikum/tillikum-core-module/issues",
        "source": "https://github.com/tillikum/tillikum-core-module"
    }
}
